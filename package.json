{
  "name": "custom-touchbar",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "app/main.js",
  "scripts": {
    "dev": "nodemon --exec 'npm start' -e ts",
    "start": "tsc && electron .",
    "postinstall": "electron-builder install-app-deps",
    "build": "tsc && cp close-icon.png app && rm -rf dist && electron-builder build"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^32.0.2",
    "electron-builder": "^25.0.5"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "openai": "^4.58.2"
  }
}
